Sub Void
  Function_UI_Add = Function_UI_Add 
  Function_RunMod_Parser = Function_RunMod_Parser
  ModPath = ModPath 
  args = args 
EndSub 

Sub Main
  LDCall.Function5( Function_UI_Add , "Menu" , "Mod" , Function_RunMod_Parser , "Main" , "Test UI.Handler" )
  LDCall.Function5( Function_UI_Add , "Menu" , "Run" , Function_RunMod_Parser , "Mod" , "Test UI.Handler" )
EndSub

Sub Handler
  LastItem = LDControls.LastMenuItem
  If LastItem = "Run" Then
    LDDialogs.OpenFile("exe", ModPath  ) 
    EndIf 
  'GraphicsWindow.ShowMessage ( args + LDControls.LastMenuItem ,"Test UI Mod" )
  
EndSub